{
	".gviMiniPhoto": {
		"width": 40,
		"height": 40,
		"paddingBottom": 3,
		"paddingTop": 3,
		"paddingLeft": 3,
		"paddingRight": 3,
		"&-img": {
			"imageFillType": "ASPECTFILL",
            "flexProps": {
			    "flexGrow": 1,
                "positionType": "RELATIVE"
            }
		},
		"&-flex": {
			"paddingBottom": 1,
			"paddingTop": 1,
			"paddingLeft": 1,
			"paddingRight": 1,
			"borderRadius": 1,
			"&-active": {
				"borderWidth": 1,
				"borderColor": "#00a1f1"
			},
            "flexProps": {
			    "flexGrow": 1,
                "positionType": "RELATIVE"
            }
		}
	}
}
