{
    "application.confirmExit": "ifRequired",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/scripts/generated/**": true,
        "**/.ui/**": true
    },
    "workspace.preserveWindow": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.autoSave": "off"
}